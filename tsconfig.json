{
    "compileOnSave": true,
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "watch": true,
        "outDir": "./wwwroot/**/",
    },
    "include": [
        "./app/**/*.ts",
    ],
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}